# This is Git's per-user configuration file.
[user]
	name = MiguelTFD
	email = MiguelTFD@outlook.com
# Please adapt and uncomment the following lines:
#	name = MiguelTFD
#	email = migueltfd@Eve.(none)
[alias]
	cl = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[init]
	defaultBranch = main
[core]
	editor = vim
[pull]
	# ff = only
	rebase = true
